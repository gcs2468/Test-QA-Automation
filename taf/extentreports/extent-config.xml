<?xml version="1.0" encoding="UTF-8"?>
    <extentreports>
    	<configuration>
    		<!-- report theme -->
    		<!-- standard, dark -->
    		<theme>dark</theme>
     
    		<!-- document encoding -->
    		<!-- defaults to UTF-8 -->
    		<encoding>UTF-8</encoding>
     
    		<!-- protocol for script and stylesheets -->
    		<!-- defaults to https -->
    		<protocol>https</protocol>
     
    		<!-- title of the document -->
    		<documentTitle>TAF 2.0 Report</documentTitle>
     
    		<!-- report name - displayed at top-nav -->
    		<reportName>
    		 <![CDATA[
                    <img style='margin: 1px;' src='./extentreports/css/TAF_01.png' alt='TAF logo' />
                ]]>
    		</reportName>
     
    		<!-- report headline - displayed at top-nav, after reportHeadline -->
    		<reportHeadline></reportHeadline>
     
    		<!-- global date format override -->
    		<!-- defaults to yyyy-MM-dd -->
    		<dateFormat>yyyy-MM-dd</dateFormat>
     
    		<!-- global time format override -->
    		<!-- defaults to HH:mm:ss -->
    		<timeFormat>HH:mm:ss</timeFormat>
     
    		<!-- custom javascript -->
    		<scripts>
          <![CDATA[
            $(document).ready(function() {
              $(".logo-content").hide();
              $(".logo-container").html("TAF 2.0");
            });
          ]]>
    		</scripts>
     
    		<!-- custom styles -->
    		<styles>
                <![CDATA[
                    .report-name { padding-left: 1px; } .report-name > img { float: left;height: 40px;margin-left: 50px;margin-top: 5px;width: 172px; }
                     nav, nav .nav-wrapper {background: #dddddd;}
                     .logo-container {text-align: left;color: #fdfefe;font-weight: bold;background: #dddddd;}
                     .nav-right {background:#dddddd;}
                     .nav-right li {background:#dddddd;}
                     .nav-right li:last-child {background:#dddddd;}

                ]]>
    		</styles>
    	</configuration>
    </extentreports>